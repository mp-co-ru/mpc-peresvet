        "update": {
          "code": "let payload = {\n  data: []\n}\ncontext.panel.elements.forEach((element) => {\n  if (element.value !== context.panel.initial[element.id]) {\n    payload.data.push({\n      tagId: element.id,\n      data: [\n        [element.value]\n      ]\n    });\n  }\n});\n\nif (payload.data.length > 0) {\n  url = window.location.protocol + \"//\" + window.location.hostname + \"/v1/data/\";\n  fetch(url, {\n    method: \"POST\",\n    body: JSON.stringify(payload),\n    headers: new Headers({\n      'Content-Type': 'application/json'\n    })\n  }).then((response) => {\n    if (!response.ok) {\n      console.error(`Ошибка записи значений ${payload}`);\n    } else {\n      console.log(\"Данные успешно записаны.\");\n      //context.panel.initialRequest();\n    }\n  });\n}",
